<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="790.0" prefWidth="1019.0">
         <children>
            <VBox layoutY="-2.0" prefHeight="796.0" prefWidth="1021.0">
               <children>
                  <HBox alignment="BOTTOM_CENTER" prefHeight="521.0" prefWidth="1021.0">
                     <children>
                        <Label text="Greetings! Would you like to purchase a cruise ticket for two?" />
                     </children>
                     <padding>
                        <Insets bottom="15.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="TOP_CENTER" prefHeight="491.0" prefWidth="1021.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#yesButtonClicked" text="Yes" />
                        <Button mnemonicParsing="false" onAction="#noButtonClicked" text="No" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</GridPane>
